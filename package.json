    "morgan": "~1.6.1",
    "normalize.css": "^3.0.3",
    "q": "^1.4.1",
    "q-io": "^1.13.2",
    "querystring": "^0.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-engine": "^3.1.0",
    "react-router": "^2.0.0-rc5",
    "serve-favicon": "~2.3.0",
